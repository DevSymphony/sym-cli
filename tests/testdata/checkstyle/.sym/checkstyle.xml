<?xml version="1.0"?>
<!DOCTYPE module PUBLIC
    "-//Checkstyle//DTD Checkstyle Configuration 1.3//EN"
    "https://checkstyle.org/dtds/configuration_1_3.dtd">
<module name="Checker">
  <module name="TreeWalker">
    <module name="MethodName">
      <property name="severity" value="error"></property>
      <property name="format" value="^[a-z][a-zA-Z0-9]*$"></property>
    </module>
    <module name="TypeName">
      <property name="severity" value="error"></property>
      <property name="format" value="^[A-Z][a-zA-Z0-9]*$"></property>
    </module>
    <module name="MemberName">
      <property name="severity" value="error"></property>
      <property name="format" value="^m_[a-z][a-zA-Z0-9]*$"></property>
      <property name="applyToPrivate" value="true"></property>
      <property name="applyToPublic" value="false"></property>
      <property name="applyToProtected" value="false"></property>
      <property name="applyToPackage" value="false"></property>
    </module>
  </module>
</module>
